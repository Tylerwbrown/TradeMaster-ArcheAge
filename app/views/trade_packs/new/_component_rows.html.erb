<%= f.simple_fields_for :component_ids, html: { class: 'col-sm-12 row' }, defaults: { wrapper_class: @presenter.component_classes } do |i| %>
  <%= @presenter.component_count.times.map { |r| (i.input r, label: "Component #{r+1}", collection: @presenter.components, as: :select) }.join.html_safe %>
<% end %>
