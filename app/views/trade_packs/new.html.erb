<h1 class="text-center">New Trade Pack</h1>
<%= simple_form_for(@trade_pack, html: { class: 'form-horizontal' } ) do |f| %>
  <%= f.input :name %>
  <%= f.input :labor_cost %>

  <%= f.submit 'Create Trade Pack', class: 'btn btn-success btn-block' %>
<% end %>

<%= link_to 'Back', root_path, class: 'btn' %>
